{
  "expo": {
    "name": "TempNow",
    "slug": "TempNow",
    "version": "1.0.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "githubUrl":"https://github.com/H1gor1248/TempNow",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#E5DCDC"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.h1gor.tempnow"
      
    },
    "android": {
      "package": "com.h1gor.tempnow",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#E5DCDC"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
